<!DOCTYPE html>
<html>
<head>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>

  <%= csrf_meta_tags %>
  <%= yield :head %>
  <%= yield :stylesheets %>
  <%= yield :title %>
</head>
<body data-no-turbolink>

<!-- Yield content -->
<%= yield %>

<!-- Yield javascript here for performance -->
<%= yield :javascripts %>
</body>
</html>